{
  "bank": {
    "table": "bank as bank",
    "fields": "bank_code,bank_name,short_name,ac_no_length,CASE WHEN checked_ac_no = 1 THEN 'YES' ELSE 'NO' END AS checked_ac_no,is_active,CASE WHEN nationalized_bank=1 THEN 'YES' ELSE 'NO' END AS nationalized_bank",
    "orderby": "bank.bank_name"
  },

  "bank_district_mapping": {
    "table": "bank_district_mapping as bdm",
    "fields": "bank_district_mapping_code,states.state_code,states.state_name,district.district_code,district.district_name,bank.bank_code,bank.bank_name",
    "join": [
      {
        "table": "bank",
        "condition": "bdm.bank_code=bank.bank_code"
      },
      {
        "table": "states",
        "condition": "bdm.state_code=states.state_code"
      },
      {
        "table": "district",
        "condition": "bdm.district_code=district.district_code"
      }
    ],
    "require": "bdm.bank_code"
  },

  "branch": {
    "table": "branch as branch",
    "fields": "branch.bank_Code,bank_name,branch.branch_code,branch.branch_name,branch.ifsc,branch.wef_date,branch.is_active",
    "join": [
      {
        "table": "bank",
        "condition": "bank.bank_code=branch.bank_code"
      }
    ],
    "require": "branch.bank_code",
    "orderby": "branch.branch_code"
  },

  "company_group": {
    "table": "company_group",
    "fields": "company_group_code,company_group_name,company_group_ex_code,company_group_short_name",
    "orderby": "company_group_code"   
  },

  "company": {
    "table": "company",
    "fields": "company.company_group_code,company_group_name,company_code,company_name,company_ex_code,company_short_name",
    "join": [
      {
        "table": "company_group",
        "condition": "company_group.company_group_code=company.company_group_code"
      }
    ],
    "orderby": "company.company_name"
  },

  "plant": {
    "table": "plant",
    "fields": "plant.plant_code,search_hierarchy.plant_tr_code as plant_transaction_code,plant.plant_name,plant.is_active,plant.plant_ex_code,company.company_name,district_name,tehshil_name,village_name,hamlet_name,bcd.mob_no",
    "join": [
      {
        "table": "business_hierarchy as search_hierarchy",
        "condition": "search_hierarchy.plant_code=plant.plant_code and search_hierarchy.mcc_code is null and search_hierarchy.is_default=1"
      },
      {
        "table": "user_organization_mapping uom",
        "condition": "search_hierarchy.plant_code=isnull(uom.plant_code,search_hierarchy.plant_code)  and uom.user_code = @rls_user_code"
      },
      {
        "table": "company",
        "condition": "company.company_code=search_hierarchy.company_code"
      },
      {
        "type": "left",
        "table": "business_address_detail ba",
        "condition": "ba.module_code=plant.plant_code and ba.module_name='plant' and ba.is_default='1'"
      },
      {
        "type": "left",
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      },
      {
        "type": "left",
        "table": "business_contact_detail as bcd",
        "condition": "bcd.module_code=plant.plant_code and bcd.module_name='plant'"
      }
    ],
    "require": "search_hierarchy.company_code",
    "orderby": "plant.plant_name",
    "distinct": "distinct"
  },

  "mcc": {
    "table": "mcc as mcc",
    "fields": "plant.plant_code,search_hierarchy.plant_tr_code as plant_transaction_code,plant.plant_name,mcc.mcc_code,search_hierarchy.mcc_tr_code as mcc_transaction_code,mcc.mcc_name,mcc.is_active,mcc.mcc_ex_code, district_name,tehshil_name,village_name,hamlet_name,bcd.mob_no",
    "join": [
      {
        "table": "business_hierarchy as search_hierarchy",
        "condition": "search_hierarchy.mcc_code=mcc.mcc_code and search_hierarchy.bmc_code is null and search_hierarchy.is_default=1"
      },
      {
        "table": "user_organization_mapping uom",
        "condition": "search_hierarchy.plant_code=isnull(uom.plant_code,search_hierarchy.plant_code) and search_hierarchy.mcc_code=isnull(uom.mcc_code,search_hierarchy.mcc_code) and  uom.user_code=@rls_user_code"
      },
      {
        "table": "plant",
        "condition": "plant.plant_code=search_hierarchy.plant_code"
      },
      {
        "type": "left",
        "table": "business_address_detail ba",
        "condition": "ba.module_code=mcc.mcc_code and ba.module_name='mcc' and ba.is_default='1'"
      },
      {
        "type": "left",
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      },
      {
        "type": "left",
        "table": "business_contact_detail bcd",
        "condition": "bcd.module_code=mcc.mcc_code and bcd.module_name='mcc'"
      }
    ],
    "require": "search_hierarchy.company_code,search_hierarchy.plant_code",
    "orderby": "mcc.mcc_name",
    "distinct": "distinct"
  },

  "bmc": {
    "table": "bmc as bmc",
    "fields": "plant.plant_code,search_hierarchy.plant_tr_code as plant_transaction_code,plant.plant_name,mcc.mcc_code,search_hierarchy.mcc_tr_code as mcc_transaction_code,mcc.mcc_name,bmc.bmc_code,search_hierarchy.bmc_tr_code as bmc_transaction_code,bmc.bmc_name,bmc.is_active,bmc.bmc_ex_code,district_name,tehshil_name,village_name,hamlet_name,bcd.mob_no",
    "join": [
      {
        "table": "business_hierarchy as search_hierarchy",
        "condition": "search_hierarchy.bmc_code=bmc.bmc_code and search_hierarchy.route_code is null and search_hierarchy.is_default=1"
      },
      {
        "table": "user_organization_mapping uom",
        "condition": "search_hierarchy.plant_code=isnull(uom.plant_code,search_hierarchy.plant_code) and search_hierarchy.mcc_code=isnull(uom.mcc_code,search_hierarchy.mcc_code) and search_hierarchy.bmc_code=isnull(uom.bmc_code,search_hierarchy.bmc_code) and uom.user_code=@rls_user_code"
      },
      {
        "table": "plant",
        "condition": "plant.plant_code=search_hierarchy.plant_code"
      },
      {
        "table": "mcc",
        "condition": "mcc.mcc_code=search_hierarchy.mcc_code"
      },
      {
        "type": "left",
        "table": "business_address_detail ba",
        "condition": "ba.module_code=bmc.bmc_code  and ba.module_name='bmc' and ba.is_default='1'"
      },
      {
        "type": "left",
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      },
      {
        "type": "left",
        "table": "business_contact_detail bcd",
        "condition": "bcd.module_code=bmc.bmc_code  and bcd.module_name='bmc'"
      }
    ],
    "require": "search_hierarchy.company_code,search_hierarchy.plant_code,search_hierarchy.mcc_code",
    "orderby": "bmc.bmc_name",
    "distinct": "distinct"
  },

  "registration": {
    "table": "registration as registration",
    "fields": "registration_details_code,registration_date,registration_no,gst_no,fssiai,msme_no ,pan_no,service_tax,tan_no,wef_date,is_default,tds_per,security_amount,security_cheque_no",
    "require": "module_name,module_code",
    "orderby": "registration.registration_details_code" 
  },

  "business_address_detail": {
    "table": "business_address_detail ba",
    "fields": "address_details_code,ba.state_code,state_name,ba.district_code,district_name,ba.tehshil_code,tehshil_name,ba.village_code,village_name,ba.hamlet_code,hamlet_name,address,city,pincode,fax_no,phone_no,email_id,websites,ba.wef_date",
    "join": [
      {
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.state_code=ba.state_code and geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      }
    ],
    "require": "module_name,module_code",
    "orderby": "address_details_code"
  },

  "business_bank_detail": {
    "table": "business_bank_detail bd",
    "fields": "bank_detail_code,bd.bank_code,bank.bank_name,bd.branch_code,branch.branch_name,accountant_name,bank_account_no,bd.ifsc,upi,bd.wef_date",
    "join": [
      {
        "table": "bank",
        "condition": "bd.bank_code=bank.bank_code"
      },
      {
        "table": "branch",
        "condition": "bd.branch_code=branch.branch_code"
      }
    ],
    "require": "module_name,module_code",
    "orderby": "bank_detail_code"
  },

  "business_contact_detail": {
    "table": "business_contact_detail",
    "fields": "contact_details_code,first_name,last_name as middle_name,sur_name,mob_no,business_contact_detail.department_code,department.department_name as department,designation,pan_no,aadhar_no,wef_date,is_default,driving_license_no,license_expiry_date,billing_type_code,agreement_from_date,agreement_no,agreement_to_date,declaration",
    "join": [
      {
        "type": "left",
        "table": "department as department",
        "condition": "department.department_code=business_contact_detail.department_code"
      }
    ],
    "require": "module_name,module_code",
    "orderby": "business_contact_detail.contact_details_code"
  },

  "mpp": {
    "table": "mpp",
    "fields": "search_hierarchy.plant_code,plant_tr_code as plant_transaction_code,plant_name,search_hierarchy.mcc_code,mcc_tr_code as mcc_transaction_code,mcc_name,search_hierarchy.bmc_code,bmc_tr_code as bmc_transaction_code,bmc_name,route_name,mpp.mpp_code,mpp_tr_code as mpp_transaction_code,mpp.mpp_ex_code,mpp.mpp_name,mpp.mpp_short_name,district_name,tehshil_name,village_name,hamlet_name,bcd.mob_no",
    "join": [
      {
        "table": "search_hierarchy",
        "condition": "search_hierarchy.mpp_code=mpp.mpp_code and search_hierarchy.is_default=1"
      },
      {
        "table": "user_organization_mapping uom",
        "condition": "search_hierarchy.plant_code=isnull(uom.plant_code,search_hierarchy.plant_code) and search_hierarchy.mcc_code=isnull(uom.mcc_code,search_hierarchy.mcc_code) and search_hierarchy.bmc_code=isnull(uom.bmc_code,search_hierarchy.bmc_code) and search_hierarchy.mpp_code=isnull(uom.mpp_code,search_hierarchy.mpp_code) and uom.user_code=@rls_user_code"
      },
      {
        "type": "left",
        "table": "business_address_detail ba",
        "condition": "ba.module_code=mpp.mpp_code and ba.module_name='mpp' and ba.is_default='1'"
      },
      {
        "type": "left",
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      },
      {
        "type": "left",
        "table": "business_contact_detail bcd",
        "condition": "bcd.module_code=mpp.mpp_code and bcd.module_name='mpp' and bcd.is_default='1'"
      }
    ],
    "require": "search_hierarchy.company_code,search_hierarchy.plant_code,search_hierarchy.mcc_code,search_hierarchy.bmc_code",
    "orderby": "mpp.mpp_name",
    "distinct": "distinct"
  },

  "collection_point": {
    "table": "collection_point as cp",
    "fields": "mcc_code,mcc_tr_code as mcc_transaction_code,mcc_name,bmc_code,bmc_tr_code as bmc_transaction_code,bmc_name,search_hierarchy.mpp_code,mpp_tr_code as mpp_transaction_code,mpp_name,cp.collection_point_code,cp.is_active,cp.wef_date",
    "join": [
      {
        "table": "search_hierarchy",
        "condition": "search_hierarchy.mpp_code=cp.mpp_code and search_hierarchy.is_default=1"
      }
    ],
    "require": "search_hierarchy.company_code,search_hierarchy.plant_code,search_hierarchy.mcc_code,search_hierarchy.bmc_code,search_hierarchy.mpp_code",   
  },

  "party_group": {
    "table": "party_group",
    "fields": "party_group.party_group_code,party_group.party_group_name,party_group.is_active",
    "orderby": "party_group.party_group_code"   
  },

  "party_master": {
    "table": "party_master as party_master",
    "fields": "party_master.party_master_code,party_master.party_name,party_master.company_code,company_name,district_name,tehshil_name,village_name,hamlet_name,bcd.mob_no",
    "join": [
      {
        "table": "company ",
        "condition": "company.company_code=party_master.company_code "
      },
      {
        "type": "left",
        "table": "business_address_detail ba",
        "condition": "ba.module_code=party_master.party_master_code and ba.module_name='party_master' and is_default='1' "
      },
      {
        "type": "left",
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      },
      {
        "type": "left",
        "table": "business_contact_detail bcd",
        "condition": "bcd.module_code=party_master.party_master_code  and bcd.module_name='party_master'"
      }
    ]
  },

  "sahayak": {
    "table": "sahayak as sahayak",
    "fields": "sahayak.sahayak_code,sahayak.sahayak_name,sahayak.sahayak_ex_code,mpp.mpp_name,district_name,tehshil_name,village_name,hamlet_name,mcd.mob_no",
    "join": [
      {
        "table": "member_sahayak_hierarchy as search_hierarchy",
        "condition": "search_hierarchy.sahayak_code=sahayak.sahayak_code "
      },
      {
        "table": "mpp as mpp",
        "condition": "mpp.mpp_code=search_hierarchy.mpp_code"
      },
      {
        "type": "left",
        "table": "member_sahayak_address_detail ba",
        "condition": "ba.module_code=sahayak.sahayak_code and ba.module_name='sahayak'  and ba.is_default='1'"
      },
      {
        "type": "left",
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      },
      {
        "type": "left",
        "table": "member_sahayak_contact_detail mcd",
        "condition": "mcd.module_code=sahayak.sahayak_code and ba.module_name='sahayak' "
      }
    ],
    "require": "search_hierarchy.company_code,search_hierarchy.plant_code,search_hierarchy.mcc_code,search_hierarchy.bmc_code,search_hierarchy.mpp_code"   
  },

  "member_sahayak_address_detail": {
    "table": "member_sahayak_address_detail ba",
    "fields": "address_details_code,ba.state_code,state_name,ba.district_code,district_name,ba.tehshil_code,tehshil_name,ba.village_code,village_name,ba.hamlet_code,hamlet_name,address,city,pincode,fax_no,phone_no,email_id,websites,wef_date",
    "join": [
      {
        "table": "geo_hierarchy_view",
        "condition": "geo_hierarchy_view.state_code=ba.state_code and geo_hierarchy_view.district_code=ba.district_code and geo_hierarchy_view.tehshil_code=ba.tehshil_code and geo_hierarchy_view.hamlet_code=ba.hamlet_code"
      }
    ],
    "require": "module_name,module_code",
    "orderby": "address_details_code"

  },

  "member_sahayak_bank_detail": {
    "table": "member_sahayak_bank_detail bd",
    "fields": "bank_detail_code,bd.bank_code,bank.bank_name,bd.branch_code,branch.branch_name,accountant_name,bank_account_no,bd.ifsc,upi,bd.wef_date",
    "join": [
      {
        "table": "bank",
        "condition": "bd.bank_code=bank.bank_code"
      },
      {
        "table": "branch",
        "condition": "bd.branch_code=branch.branch_code"
      }
    ],
    "require": "module_name,module_code",
    "orderby": "bank_detail_code"
  },

  "member_sahayak_contact_detail": {
    "table": "member_sahayak_contact_detail",
    "fields": "contact_details_code,first_name,last_name as middle_name,sur_name,department.department_code,department_name as department,designation,pan_no,aadhar_no,wef_date,is_default,driving_license_no,license_expiry_date,billing_type_code,agreement_from_date,agreement_no,agreement_to_date,declaration",
    "join": [
      {
        "type": "left",
        "table": "department",
        "condition": "department.department_code=business_contact_detail.department_code"
      }
    ],
    "require": "module_name,module_code",
    "orderby": "contact_details_code"
  },

  "vehicle_master": {
    "table": "vehicle_master as vehicle_master",
    "fields": "vehicle_code,vehicle_type,parsing_no,capacity,applicable_rto,average,rc_book_no,is_active",
    "require": "party_master_code"   
  },

  "vehicle_detail": {
    "table": "vehicle_detail as vehicle_detail",
    "fields": "vehicle_detail_code,vehicle_master_code,billing_type,insurance,pollution_certificate,rent,wef_date",
    "require": "vehicle_master_code"       
  },

  "route": {
    "table": "route as route",
    "fields": "mcc_tr_code,search_hierarchy.mcc_code,mcc_name,bmc_tr_code,search_hierarchy.bmc_code,bmc_name,route.route_code,route.route_name,route_type,right(route_ex_code,3) as route_ex_code",
    "join": [
      {
        "table": "business_hierarchy as search_hierarchy",
        "condition": "search_hierarchy.route_code=route.route_code and is_default='1' and search_hierarchy.mpp_code is null "
      },
      {
        "table": "mcc as mcc",
        "condition": "mcc.mcc_code=search_hierarchy.mcc_code"
      },
      {
        "table": "bmc as bmc",
        "condition": "bmc.bmc_code=search_hierarchy.bmc_code"
      }
    ],
    "orderby": "route_code",
    "require": "search_hierarchy.plant_code,search_hierarchy.mcc_code,search_hierarchy.bmc_code"
  },
  "route_vehicle_mapping": {
    "table": "route_vehicle_mapping",
    "fields": "route_vehicle_mapping,route_code,party_name,vehicle_master.vehicle_code,vehicle_type,parsing_no,wef_date,is_default",
    "join": [
      {
        "table": "vehicle_master",
        "condition": "vehicle_master.vehicle_code=route_vehicle_mapping.vehicle_code"
      },
      {
        "table": "party_master",
        "condition": "party_master.party_master_code=vehicle_master.party_master_code"
      }
    ],
    "require": "route_code",
    "orderby": "route_vehicle_mapping"
  },

  "route_other_info_for_km": {
    "table": "route_other_info_for_km as rkm",
    "fields": "route_other_info_km_code,morning_extra_km,evening_extra_km,morning_withmilk_km,morning_withoutmilk_km,evening_withmilk_km,evening_withoutmilk_km,r.route_code,r.route_name,wef_date",
    "join": [
      {
        "table": "route as r",
        "condition": "r.route_code=rkm.route_code"
      }
    ],
    "require": "rkm.route_code",
    "orderby": "route_other_info_km_code",
    "distinct": "distinct"
  },

  "route_other_info_for_time": {
    "table": "route_other_info_for_time",
    "fields": "route_other_info_time_code,morning_start_time,morning_end_time,morning_grace_time,evening_start_time,evening_end_time,evening_grace_time,wef_date",
    "require": "route_code",
    "orderby": "route_other_info_time_code",
    "distinct": "distinct"
  },

  "business_hierarchy": {
    "table": "business_hierarchy",
    "fields": "plant_code,plant_tr_code as plant_transaction_code,mpp_code,mcc_tr_code as mcc_transaction_code,bmc_code",
    "require": "route_code",
    "orderby": "mpp_code,plant_code,bmc_code",
    "distinct": "distinct"
  },

  "bmc_silos_info": {
    "table": "bmc_silos_info",
    "fields": "silos_code,silo_no,chilling_capacity,manufacturer_code as manufacturer_type,wef_date,model,storage_capacity,owning_type,module_code,module_name,is_default,bmc_silos_info.milk_type_code,bmc_type,milk_type.milk_type_name,description",
    "join": [
      {
        "table": "milk_type",
        "condition": "milk_type.milk_type_code=bmc_silos_info.milk_type_code"
      }
    ],
    "require": "module_code,module_name",
    "orderby": "silos_code",
    "distinct": "distinct"
  },
  "transfer_entity": {
    "table": "transfer_entity",
    "fields": "transfer_data_id,transfer_type,company_code,plant_code,from_mcc,to_mcc,from_bmc,to_bmc,from_route,to_route,from_mpp,to_mpp,member_code,wef_date,record_status",
    "require": "transfer_type"
  },
  "dpu_registration": {
    "table": "dpu_registration as dr",
    "fields": "dpu_registration_code,search_hierarchy.bmc_code,search_hierarchy.bmc_tr_code,search_hierarchy.bmc_name,search_hierarchy.mpp_code,search_hierarchy.mpp_tr_code,search_hierarchy.mpp_name,mobile_no,service_provider,sim_issued_to,sim_no,imei_no,dpu_serial_no,dr.wef_date",
    "join": [
      {
        "table": "business_hierarchy_view as search_hierarchy",
        "condition": "dr.mpp_code=search_hierarchy.mpp_code and search_hierarchy.is_default=1"
      }
    ],
    "require": "search_hierarchy.company_code,search_hierarchy.plant_code,search_hierarchy.mcc_code,search_hierarchy.bmc_code"
  },

  "reil_dpu_configuration_list": {
    "table": "reil_dpu_configuration as rdc",
