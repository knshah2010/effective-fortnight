{

  "users": {
    "table": "users",
    "fields": "users.user_code,users.user_name,users.name,users.email,users.mobile_no,users.password,users.department_code,department.department_name,users.is_desktop_user",
    "join": [
      {
        "table": "department",
        "condition": "department.department_code=users.department_code"
      }
    ],
    "require": "user_code"
  },

  "login": {
    "table": "users as users",
    "fields": "users.user_name,users.user_code,dept.department_code,dept.department_name",
    "join": [
      {
        "table": "department dept",
        "condition": "dept.department_code=users.department_code"
      }
    ],
    "require": "users.user_name,users.password"
  }


}
