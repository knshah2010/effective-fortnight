{
  "generalconfigurationtxn": {
    "Fields": "concat(configuration_result_key,'##',configuration_result) as configuration_result",
    "Table": "general_configuration_txn",
    "require": "configuration_key"
  },
  "generalconfigurationtxn_inner": {
    "Fields": "concat(configuration_result_key,'##',configuration_result) as configuration_result",
    "Table": "general_configuration_txn",
    "require": "configuration_key",
    "where": [
      {
        "propertyName": "configuration_key",
        "propertyValue": "?"

      }
    ]
  },
  "approval_stage_master": {
    "table": "approval_stage_master",
    "fields": "approval_stage_master_code,approval_type,stages as levels,approval_mode,wef_date",
    "require": "approval_stage_master_code"
  },
  "applicable_stage": {
    "directquery": "select approval_stage_master_code from approval_stage_master",
    "where": [
      {
        "direct_condition": "wef_date=(select max(wef_date) from approval_stage_master where approval_type=@approval_type) ",
        "propertyvalue": "#$#",
        "propertyname": "default"
      }
    ],
    "require": "approval_type"
  },

  "financial_year": {
    "table": "financial_year",
    "fields": "financial_year_code,financial_year,start_date,end_date,is_active",
    "require": "financial_year_code"
  }
}
