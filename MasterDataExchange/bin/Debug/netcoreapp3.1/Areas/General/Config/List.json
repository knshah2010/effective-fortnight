{
  "users": {
    "model": "Authentication.Areas.Login.Models",
    "fields": "users.company_code,users.user_code,users.user_name,users.name,users.email,users.mobile_no,users.password,users.department_code,department.department_name",
    "join": [
      {
        "table": "department",
        "condition": "department.department_code=users.department_code"
      }
    ]
  }
}
